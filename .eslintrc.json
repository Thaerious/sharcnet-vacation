{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "quotes": ["error", "backtick"],
        "indent": ["error", 4],
        "semi": ["error", "always"],
        "max-len": ["error", {"code": 120}]
    }
}
